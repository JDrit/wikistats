<br>
<br>
<div class="row text-center">
    <div class="col-md-6">
        <a href="http://en.wikipedia.org/wiki/<%= @page_title %>">
            <h1 id="page_title"><%= @page_title.tr("_", " ") %></h1>
        </a>
    </div>
    <div class="col-md-6">
        <img src="<%= @image_url %>" class="img-rounded">
    </div>
</div>
<button id="group-by-hour" class="btn btn-default">Group by hour</button>
<button id="group-by-day" class="btn btn-default">Group by day</button>

<div id="view-graph" style="height: 400px; min-width: 310px"></div>

<%= javascript_tag do %>
    window.page_title = '<%= @page_title %>';
<% end %>

